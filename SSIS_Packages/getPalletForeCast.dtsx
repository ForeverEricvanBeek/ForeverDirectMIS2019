              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH OPEN_ORDERS AS (
  SELECT 
    A.DSG_SHIP_VIA
    , CASE WHEN B.TC_SHIPMENT_ID IS NULL THEN A.TC_SHIPMENT_ID ELSE B.TC_SHIPMENT_ID END AS TC_SHIPMENT_ID
    , COUNT(DISTINCT A.TC_ORDER_ID) AS ORDERS
  FROM ORDERS A
  LEFT JOIN LPN B ON B.ORDER_ID = A.ORDER_ID
  WHERE A.ORDER_TYPE  = 'OF' 
  AND A.IS_CANCELLED = 0 
  AND A.CREATED_DTTM &gt;= SYSDATE-14 
  AND A.DO_STATUS &lt; 190 
  AND A.DO_STATUS &gt; 110
  GROUP BY
     A.DSG_SHIP_VIA
     , CASE WHEN B.TC_SHIPMENT_ID IS NULL THEN A.TC_SHIPMENT_ID ELSE B.TC_SHIPMENT_ID END
)
, MES AS ( -- Weight by oLPN type
  SELECT
    *
  FROM (
    SELECT
       P1.CODE_DESC                                                                                AS SHIP_VIA
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,32,4) AS INT)         AS EV1 
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,36,4) AS INT)         AS XSM 
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,40,4) AS INT)         AS SML 
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT)         AS STD 
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,48,4) AS INT)         AS XL
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,48,4) AS INT)         AS XLC
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,48,4) AS INT)         AS XLB
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,48,4) AS INT)         AS XLD
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,48,4) AS INT)         AS BXL
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,52,4) AS INT)         AS KIT
       , CAST(SUBSTR(P2.MISC_FLAGS,44,4) AS INT) / CAST(SUBSTR(P2.MISC_FLAGS,48,4) AS INT)         AS XLF
    FROM          SYS_CODE P1 
    LEFT JOIN     SYS_CODE P2 
    ON            P2.CODE_TYPE = 'Z05' 
    AND           P2.REC_TYPE = 'C' 
    AND           P2.CODE_ID = SUBSTR(P1.MISC_FLAGS,16,5)
    WHERE         P1.CODE_TYPE = 'Z02' 
    AND           P1.REC_TYPE = 'C' 
   ) A 
  UNPIVOT (VAL FOR TYPE IN (EV1 AS 'EV1', XSM AS 'XSM', SML AS 'SML', STD AS 'STD', XL AS 'XL', XLC AS 'XLC', XLB AS 'XLB', XLD AS 'XLD', BXL AS 'BXL', KIT AS 'KIT', XLF AS 'XLF'))
)
, AVGT AS ( -- Calculate avarage oLPN type weight by Ship Via
SELECT
  X.DSG_SHIP_VIA
  , X.CODE_ID
  , X.TVALUE AS TVALUE1 -- oLPN last 30 days by box size
  , Y.TVALUE AS TVALUE2 -- oLPN last 30 days total
  , CASE WHEN NVL(X.TVALUE / Y.TVALUE,1) = 0 THEN 0 ELSE NVL(X.TVALUE / Y.TVALUE,1) END AS AVA -- Weight
  , X.WEIGHT AS WEIGHT1
  , Y.WEIGHT AS WEIGHT2
FROM (
  SELECT
    A.DSG_SHIP_VIA
    , NVL(B.CONTAINER_SIZE, 'STD') AS CODE_ID
    , COUNT(NVL(B.CONTAINER_SIZE, 'STD')) AS TVALUE
    , AVG(B.ESTIMATED_WEIGHT) AS WEIGHT
  FROM ORDERS A 
  INNER JOIN LPN B ON B.ORDER_ID = A.ORDER_ID 
  WHERE A.CREATED_DTTM &gt; TRUNC(ADD_MONTHS(SYSDATE, -1)) 
  AND A.CREATED_DTTM &lt; TRUNC(SYSDATE) 
  AND A.ORDER_TYPE = 'OF' 
  AND A.IS_CANCELLED = 0
  HAVING COUNT(NVL(B.CONTAINER_SIZE, 'STD')) &gt; 0
  GROUP BY A.DSG_SHIP_VIA, NVL(B.CONTAINER_SIZE, 'STD')
  ) X
  LEFT JOIN (
        SELECT
          D.DSG_SHIP_VIA
          , COUNT(E.TC_LPN_ID) AS TVALUE
          , AVG(E.ESTIMATED_WEIGHT) AS WEIGHT
        FROM ORDERS D 
        INNER JOIN LPN E ON E.ORDER_ID = D.ORDER_ID 
        WHERE D.CREATED_DTTM &gt; TRUNC(ADD_MONTHS(SYSDATE, -1)) 
        AND D.CREATED_DTTM &lt; TRUNC(SYSDATE) 
        AND D.ORDER_TYPE = 'OF' 
        AND D.IS_CANCELLED = 0
        GROUP BY D.DSG_SHIP_VIA
     ) Y ON Y.DSG_SHIP_VIA = X.DSG_SHIP_VIA
)
, AVG2 AS ( -- Calculate avarage oLPN's per order by Ship Via
  SELECT
   X.DSG_SHIP_VIA
   , MEDIAN(X.TVALUE) AS TVALUE
  FROM (
    SELECT
      A.DSG_SHIP_VIA
      , TRUNC(A.CREATED_DTTM) AS DATUM
      , COUNT(B.TC_LPN_ID) AS OLPN
      , COUNT(DISTINCT B.TC_ORDER_ID) AS "ORDER"
      , ROUND((CASE WHEN COUNT(B.TC_LPN_ID) &gt; 0 THEN (COUNT(B.TC_LPN_ID) / COUNT(DISTINCT B.TC_ORDER_ID)) ELSE 0 END), 2) AS TVALUE
    FROM ORDERS A 
    INNER JOIN LPN B ON B.ORDER_ID = A.ORDER_ID 
    WHERE A.CREATED_DTTM &gt; TRUNC(ADD_MONTHS(SYSDATE, -1)) 
    AND A.CREATED_DTTM &lt; TRUNC(SYSDATE) 
    AND A.ORDER_TYPE = 'OF' 
    AND A.IS_CANCELLED = 0
    GROUP BY A.DSG_SHIP_VIA, TRUNC(A.CREATED_DTTM)
    ORDER BY A.DSG_SHIP_VIA
  ) X
  GROUP BY X.DSG_SHIP_VIA
), WAVED AS (
  SELECT
    IA.SCHEDULED_DTTM
    , CC.CARRIER_CODE
    , P3.MISC_FLAGS AS GROUPSORT
    , OD.DSG_SHIP_VIA
    , (CASE WHEN LP.TC_SHIPMENT_ID IS NULL THEN OD.TC_SHIPMENT_ID ELSE LP.TC_SHIPMENT_ID END) AS SHIPMENT
    , SV.DESCRIPTION AS SHIP_VIA_DESC
    , LP.CONTAINER_SIZE AS BOX_TYPE
    , OO.ORDERS AS ORDERS
    , COUNT(LP.TC_LPN_ID) AS OLPNS_WAVED
    , COUNT(PT.CNTR_NBR) AS OLPNS_STAGED
    , CAST(NULL AS DECIMAL(18,2))   AS OLPNS_NOT_WAVED
    -- Pallet calculation
    , CAST(MS.VAL AS DECIMAL(18,2)) AS OLPN_PALLET_VOLUME                                                                             -- Box type takes up xx percent of a standard box
    , CAST(MS.VAL * COUNT(LP.TC_LPN_ID) AS DECIMAL(18,2)) AS PALLET_OLPN                                                              -- Takes up xx standard boxes
    , CAST(SUBSTR(P2.MISC_FLAGS,29,3) AS INT) AS PALLET_VOLUME                                                                                     -- Number of standard boxes on pallet
    , CAST((MS.VAL * COUNT(LP.TC_LPN_ID)) / CAST(SUBSTR(P2.MISC_FLAGS,29,3) AS INT) AS DECIMAL(18,2)) AS PALLET_NEEDED                             -- Pallets needed
    , CAST((MS.VAL * COUNT(PT.CNTR_NBR)) / CAST(SUBSTR(P2.MISC_FLAGS,29,3) AS INT) AS DECIMAL(18,2)) AS PALLET_READY                               -- Pallets ready
    -- Container calculation
    , CAST(CT.XTERNL_CNTR_VOL AS DECIMAL(18,2)) AS OLPN_VOLUME_CM3                                                                    -- Kubic centimeter volume of box type
    , CAST((COUNT(LP.TC_LPN_ID) * CT.XTERNL_CNTR_VOL) / 1000000 AS DECIMAL(18,4))  AS CONTAINER_VOLUME                                -- Volume needed in container
    , CAST(SUM(LP.ESTIMATED_WEIGHT) AS DECIMAL(18,4))  AS CONTAINER_WEIGHT
  FROM          ORDERS OD
  LEFT JOIN     LPN LP 
  ON            LP.ORDER_ID = OD.ORDER_ID
  LEFT JOIN     SHIP_VIA SV 
  ON            SV.SHIP_VIA = OD.DSG_SHIP_VIA
  LEFT JOIN     CARRIER_CODE CC 
  ON            CC.CARRIER_ID = SV.CARRIER_ID
  LEFT JOIN     SYS_CODE SC 
  ON            SC.CODE_ID = OD.DSG_SHIP_VIA 
  AND           SC.REC_TYPE = 'C' 
  AND           SC.CODE_TYPE = 'CBC'
  LEFT JOIN     ILM_APPOINTMENT_OBJECTS AO 
  ON            AO.APPT_OBJ_ID = (CASE WHEN LP.TC_SHIPMENT_ID IS NULL THEN OD.SHIPMENT_ID ELSE LP.SHIPMENT_ID END) 
  AND           AO.APPT_OBJ_TYPE = 30
  LEFT JOIN     ILM_APPOINTMENTS IA 
  ON            IA.APPOINTMENT_ID = AO.APPOINTMENT_ID
  LEFT JOIN     MES MS 
  ON            MS.SHIP_VIA = OD.DSG_SHIP_VIA
  AND           MS.TYPE = LP.CONTAINER_SIZE
  LEFT JOIN     SYS_CODE P1 
  ON            P1.CODE_TYPE = 'Z02' 
  AND           P1.REC_TYPE = 'C' 
  AND           P1.CODE_ID = OD.DSG_SHIP_VIA
  LEFT JOIN     SYS_CODE P2 
  ON            P2.CODE_TYPE = 'Z05' 
  AND           P2.REC_TYPE = 'C' 
  AND           P2.CODE_ID = SUBSTR(P1.MISC_FLAGS,16,5)
  LEFT JOIN     SYS_CODE P3 
  ON            P3.CODE_TYPE = 'Z03' 
  AND           P3.REC_TYPE = 'C' 
  AND           P3.CODE_ID = SUBSTR(P1.MISC_FLAGS,6,7)
  LEFT JOIN     OPEN_ORDERS OO 
  ON            OO.DSG_SHIP_VIA = OD.DSG_SHIP_VIA
  AND           OO.TC_SHIPMENT_ID = (CASE WHEN LP.TC_SHIPMENT_ID IS NULL THEN OD.TC_SHIPMENT_ID ELSE LP.TC_SHIPMENT_ID END)
  LEFT JOIN     CNTR_TYPE CT 
  ON            CT.CNTR_TYPE = LP.CONTAINER_TYPE AND CT.CNTR_SIZE = LP.CONTAINER_SIZE
  LEFT JOIN     (SELECT 
                   TT.CNTR_NBR 
                 FROM PROD_TRKG_TRAN TT 
                 WHERE TT.MENU_OPTN_NAME = 'RF Anchor oLPN Pltz'
                 GROUP BY 
                 TT.CNTR_NBR) PT 
  ON             PT.CNTR_NBR = LP.TC_LPN_ID
  WHERE          OD.ORDER_TYPE  = 'OF'
  AND            OD.IS_CANCELLED = 0
  AND            (IA.SCHEDULED_DTTM &gt;= TRUNC(SYSDATE)-2 OR IA.SCHEDULED_DTTM IS NULL)
  AND            OD.CREATED_DTTM &gt;= SYSDATE-14
  AND            OD.DO_STATUS &lt; 190
  AND            OD.DO_STATUS &gt; 110
  GROUP BY
    IA.SCHEDULED_DTTM
    , CC.CARRIER_CODE
    , P3.MISC_FLAGS
    , OD.DSG_SHIP_VIA
    , (CASE WHEN LP.TC_SHIPMENT_ID IS NULL THEN OD.TC_SHIPMENT_ID ELSE LP.TC_SHIPMENT_ID END)
    , SV.DESCRIPTION
    , OO.ORDERS
    , LP.CONTAINER_SIZE
    , MS.VAL
    , CAST(SUBSTR(P2.MISC_FLAGS,29,3) AS INT)
    , CT.XTERNL_CNTR_VOL
  ORDER BY
    IA.SCHEDULED_DTTM
    , CC.CARRIER_CODE
    , OD.DSG_SHIP_VIA
)
, NOTWAVED AS (
  SELECT
    X.SCHEDULED_DTTM                        AS SCHEDULED_DTTM
    , X.CARRIER_CODE                        AS CARRIER_CODE
    , P3.MISC_FLAGS                         AS GROUPSORT
    , X.DSG_SHIP_VIA                        AS DSG_SHIP_VIA
    , X.SHIPMENT                            AS SHIPMENT
    , X.SHIP_VIA_DESC                       AS SHIP_VIA_DESC
    , X.CONTAINER_SIZE                      AS BOX_TYPE
    , COUNT(DISTINCT X.ORDER_ID)            AS ORDERS
    , NULL                                  AS OLPN_WAVED
    , NULL                                  AS OLPN_STAGED
    , CAST(SUM(X.OLPN_NOT_WAVED) AS DECIMAL(18,2))                 AS OLPN_NOT_WAVED
    -- Pallet calculation
    , CAST(MS.VAL AS DECIMAL(18,2))                                AS OLPN_PALLET_VOLUME                                              -- Box type takes up xx percent of a standard box
    , CAST(MS.VAL * SUM(X.OLPN_NOT_WAVED) AS DECIMAL(18,2))        AS PALLET_OLPN                                                     -- Takes up xx standard boxes
    , CAST(SUBSTR(P2.MISC_FLAGS,29,3) AS INT)                      AS PALLET_VOLUME                                                   -- Number of standard boxes on pallet
    , CAST((MS.VAL * SUM(X.OLPN_NOT_WAVED)) / CAST(SUBSTR(P2.MISC_FLAGS,29,3) AS INT) AS DECIMAL(18,2)) AS PALLET_NEEDED              -- Pallets needed
    , CAST(0 AS DECIMAL(18,2)) AS PALLET_READY
     -- Container calculation
    , CAST(CT.XTERNL_CNTR_VOL AS DECIMAL(18,2))                    AS OLPN_VOLUME_CM3                                                 -- Kubic centimeter volume of box type
    , CAST((SUM(X.OLPN_NOT_WAVED) * CT.XTERNL_CNTR_VOL) / 1000000 AS DECIMAL(18,4))  AS CONTAINER_VOLUME
    , CAST(SUM(X.CONTAINER_WEIGHT) AS DECIMAL(18,4)) AS AVERAGE_WEIGHT                             -- Volume needed in container
  FROM (
    SELECT
       NULL                                 AS SCHEDULED_DTTM
       , CC.CARRIER_CODE                    AS CARRIER_CODE
       , OD.DSG_SHIP_VIA                    AS DSG_SHIP_VIA
       , OD.TC_SHIPMENT_ID                  AS SHIPMENT
       , SV.DESCRIPTION                     AS SHIP_VIA_DESC
       , OD.TC_ORDER_ID                     AS ORDER_ID
       , CT.CNTR_TYPE                       AS CONTAINER_TYPE
       , CT.CNTR_SIZE                       AS CONTAINER_SIZE
       , A2.TVALUE * A1.AVA                 AS OLPN_NOT_WAVED
       , A1.WEIGHT1                         AS CONTAINER_WEIGHT
    FROM              ORDERS OD
    LEFT JOIN         SHIP_VIA SV 
    ON                SV.SHIP_VIA = OD.DSG_SHIP_VIA
    LEFT JOIN         CARRIER_CODE CC 
    ON                CC.CARRIER_ID = SV.CARRIER_ID
    LEFT JOIN         (SELECT ORDER_ID, MAX(LPN_TYPE) AS CONTAINER_TYPE FROM ORDER_LINE_ITEM GROUP BY ORDER_ID) OL
    ON                OL.ORDER_ID = OD.ORDER_ID
    LEFT JOIN         CNTR_TYPE CT 
    ON                CT.CNTR_TYPE = OL.CONTAINER_TYPE
    LEFT JOIN         AVGT A1 
    ON                A1.DSG_SHIP_VIA = OD.DSG_SHIP_VIA 
    AND               A1.CODE_ID = CT.CNTR_SIZE
    LEFT JOIN         AVG2 A2 
    ON                A2.DSG_SHIP_VIA = OD.DSG_SHIP_VIA
    WHERE             OD.ORDER_TYPE  = 'OF'
    AND               OD.IS_CANCELLED = 0
    AND               OD.DO_STATUS = 110
) X
  LEFT JOIN           MES MS 
  ON                  MS.SHIP_VIA = X.DSG_SHIP_VIA
  AND                 MS.TYPE = X.CONTAINER_SIZE
  LEFT JOIN           SYS_CODE P1 
  ON                  P1.CODE_TYPE = 'Z02' 
  AND                 P1.REC_TYPE = 'C' 
  AND                 P1.CODE_ID = X.DSG_SHIP_VIA
  LEFT JOIN           SYS_CODE P2 
  ON                  P2.CODE_TYPE = 'Z05' 
  AND                 P2.REC_TYPE = 'C' 
  AND                 P2.CODE_ID = SUBSTR(P1.MISC_FLAGS,16,5)
  LEFT JOIN           SYS_CODE P3 
  ON                  P3.CODE_TYPE = 'Z03' 
  AND                 P3.REC_TYPE = 'C' 
  AND                 P3.CODE_ID = SUBSTR(P1.MISC_FLAGS,6,7) 
  LEFT JOIN           CNTR_TYPE CT 
  ON                  CT.CNTR_TYPE = X.CONTAINER_TYPE 
  AND                 CT.CNTR_SIZE = X.CONTAINER_SIZE
  GROUP BY
    X.SCHEDULED_DTTM
    , X.CARRIER_CODE
    , P3.MISC_FLAGS
    , X.DSG_SHIP_VIA
    , X.SHIPMENT
    , X.SHIP_VIA_DESC
    , ''
    , X.CONTAINER_SIZE
    , MS.VAL
    , CAST(SUBSTR(P2.MISC_FLAGS,29,3) AS INT)
    , CT.XTERNL_CNTR_VOL
)

