                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH CTE_XML (XML_COLUMN) AS (
SELECT
  RO.CONTRACT           AS CONTRACT
  , RO.PART_NO          AS PART_NO   
  , RO.MACH_RUN_FACTOR  AS MACH_RUN_FACTOR
  , RO.WORK_CENTER_NO   AS WORK_CENTER_NO
  , (SELECT 
        WC.DEPARTMENT_NO AS DEPARTMENT_NO
        FROM IFS.WORK_CENTER WC
        WHERE WC.CONTRACT = 'FD01'
        AND WC.WORK_CENTER_NO = RO.WORK_CENTER_NO
		FOR XML PATH('WORKCENTER_GROUP'), ROOT('WORK_CENTER'), TYPE
  ) --AS 'DEPARTMENT_NO'
FROM      IFS.ROUTING_OPERATION RO
WHERE     RO.PHASE_OUT_DATE IS NULL 
AND       RO.CONTRACT = 'FD01'
FOR XML PATH ('WORKCENTER_CAPACITY'), ROOT('ROUTING_OPERATION')

)

SELECT CAST(XML_COLUMN AS NVARCHAR(MAX)) AS XML_COLUMN FROM CTE_XML</property>
