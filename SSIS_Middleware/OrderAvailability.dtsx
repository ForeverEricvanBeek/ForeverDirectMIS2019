              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=14.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
  X.ITEM_NAME                                                    AS ITEM_NAME
  , X.NUM_ORDERS                                                 AS NUMBER_OF_ORDERS
  , X.ORDERT                                                     AS NUMBER_OF_UNITS
  , X.ORDERT - X.STOCK                                           AS REQUIRED
  , X.STOCK                                                      AS STOCK
  , X.REPLEN_ACTIVE +  X.REPLEN_RESERVE                          AS REPLENISHMENT
  , X.RESV                                                       AS RESERVED
  , X.LOCKED                                                     AS LOCKED
  , (X.STOCK - X.ORDERT) + (X.RESV - X.LOCKED) AS AVAILABLE
FROM         (SELECT 
     OD.ITEM_NAME                                                AS ITEM_NAME
     , SUM(OD.ORDER_QTY)                                         AS ORDERT
     , COUNT(DISTINCT O.ORDER_ID)                                AS NUM_ORDERS
     , NVL(SC.AVA, '0')                                          AS STOCK
     , NVL(SC.REPLEN, '0')                                       AS REPLEN_ACTIVE
     , NVL(RC.REPLEN, '0')                                       AS REPLEN_RESERVE
     , NVL(RC.AVA, '0')                                          AS RESV
     , NVL(LC.AVA, '0')                                          AS LOCKED
  FROM         ORDERS O
  INNER JOIN   ORDER_LINE_ITEM OD
  ON           OD.ORDER_ID = O.ORDER_ID
  AND          DO_STATUS IN ('110','120')
  AND          OD.ALLOCATED_QTY = '0'
  LEFT JOIN    (SELECT 
                     NVL(( SUM(WI.ON_HAND_QTY) - SUM(WI.WM_ALLOCATED_QTY) ), '0') AS AVA
                     , NVL(SUM(WI.TO_BE_FILLED_QTY), '0')                         AS REPLEN
                     , WI.ITEM_ID                                                 AS ITEM_ID
                  FROM         WM_INVENTORY WI
                  INNER JOIN   LOCN_HDR LH
                  ON           WI.LOCATION_ID = LH.LOCN_ID
                  WHERE        WI.TC_LPN_ID IS NOT NULL
                  AND          WI.LOCN_CLASS IN ('A','C')
                  AND          LH.PICK_DETRM_ZONE NOT IN ('KIT','0','AGS','CGS')
                  GROUP BY 
                     WI.ITEM_ID) SC
  ON           SC.ITEM_ID = OD.ITEM_ID
  LEFT JOIN    (SELECT 
                     NVL(( SUM(WI.ON_HAND_QTY) - SUM(WI.WM_ALLOCATED_QTY) ), '0')AS AVA
                     , NVL(SUM(WI.WM_ALLOCATED_QTY), '0')                        AS REPLEN
                     , WI.ITEM_ID                                                AS ITEM_ID
                  FROM         WM_INVENTORY WI
                  WHERE        WI.TC_LPN_ID IS NOT NULL
                  AND          WI.LOCN_CLASS IN ('R')
                  GROUP BY 
                     WI.ITEM_ID) RC
  ON           RC.ITEM_ID = OD.ITEM_ID
  LEFT JOIN    (  SELECT 
                     NVL((SUM(WI.ON_HAND_QTY) - SUM(WI.WM_ALLOCATED_QTY)),'0')      AS AVA
                     , NVL(SUM(WI.TO_BE_FILLED_QTY), '0')                           AS REPLEN
                     , WI.ITEM_ID                                                   AS ITEM_ID
                  FROM         WM_INVENTORY WI
                  WHERE        WI.TC_LPN_ID IS NOT NULL
                  AND          WI.TC_LPN_ID IN (SELECT DISTINCT TC_LPN_ID FROM LPN_LOCK)
                  GROUP BY 
                     WI.ITEM_ID) LC
  ON           LC.ITEM_ID = OD.ITEM_ID                
  WHERE 
     ORDER_TYPE = 'OF'
  GROUP BY 
     NVL(SC.AVA, '0')
     , NVL(SC.REPLEN, '0')
     , NVL(RC.REPLEN, '0')
     , NVL(RC.AVA, '0')
     , NVL(LC.AVA, '0')
     , OD.ITEM_NAME) X

GROUP BY
  X.ITEM_NAME
  , X.NUM_ORDERS
  , X.ORDERT
  , X.ORDERT - X.STOCK  
  , X.STOCK
  , X.REPLEN_ACTIVE +  X.REPLEN_RESERVE
  , X.RESV
  , X.LOCKED
  , (X.STOCK - X.ORDERT) + (X.RESV - X.LOCKED)
HAVING 
  X.ORDERT - X.STOCK &gt; 0 
ORDER BY 
  X.ITEM_NAME</property>
